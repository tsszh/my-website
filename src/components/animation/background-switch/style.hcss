.example-enter {
  opacity: 0.3;
}

.example-enter.example-enter-active {
  opacity: 1;
  -webkit-transition: all 10s linear;
  transition: all 10s linear;
}

.example-leave {
  opacity: 1;
}

.example-leave.example-leave-active {
  opacity: 0.3;
  -webkit-transition: all 10s linear;
  transition: all 10s linear;
}

.example-appear {
  opacity: 0.3;
}

.example-appear.example-appear-active {
  opacity: 1;
  -webkit-transition: all 10s linear;
  transition: all 10s linear;
}
